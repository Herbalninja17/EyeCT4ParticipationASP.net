<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - EyeCT4Participation</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    @model tester.Models.Online
    <div class="menubar">
        <div class="menu">            
          @Html.ActionLink("Home", "Index", "Login", new { area = "" }, new { @class = "tab" })
          @Html.ActionLink("Requests", "Requests", "Needy", new { area = "" }, new { @class = "tab" })
          @Html.ActionLink("Profile", "Profile", "Login", new { area = "" }, new { @class = "tab" })
          @Html.ActionLink("Inbox", "Chatbox", "Chat", new { area = "" }, new { @class = "tab" })
          @Html.ActionLink("Sign In", "Index", "Login", new { area = "" }, new { @class = "tab" })
              
            
        </div>
    </div>
    @*<iframe width="1" height="1" wmode="transparent" src="//www.youtube.com/embed/F9vA7L8H4nc?autoplay=1&loop=1" frameborder="0"><</iframe>*@

    <div class="wrapper">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - EyeCT4Participation</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>