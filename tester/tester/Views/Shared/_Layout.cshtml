<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    @*<link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />*@
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>

    <div class="menubar">
        <div class="menu">
            @Html.ActionLink("Home", "Index", "Login", new { area = "" }, new { @class = "tab" })
            @Html.ActionLink("Requests", "Index", "Login", new { area = "" }, new { @class = "tab" })
            @*@if ( ViewBag.Titel != "Index")
                {
                    string LText = "Log Out:" + Convert.ToString(ViewBag.Titel);
                    @Html.ActionLink("Test1", "Index", "Login", new { area = "" }, new { @class = "tab" });
                    @Html.ActionLink("Test2", "Index", "Login", new { area = "" }, new { @class = "tab" });
                    @Html.ActionLink(LText, "Index", "Login", new { area = "" }, new { @class = "tab" });
                }
                @if (ViewBag.Titel == "Index")
                {
                    @Html.ActionLink("Home", "Index", "Login", new { area = "" }, new { @class = "tab" });
                }*@

        </div>
    </div>
    <iframe width="1" height="1" wmode="transparent" src="//www.youtube.com/embed/F9vA7L8H4nc?autoplay=1&loop=1" frameborder="0"><</iframe>

    <div class="wrapper">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>